import kotlin.Int;

CREATE TABLE World (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    width INTEGER AS Int NOT NULL,
    height INTEGER AS Int NOT NULL,
    cells BLOB NOT NULL
);

insert:
INSERT INTO World(width, height, cells, name)
VALUES (?, ?, ?, ?);

update:
UPDATE World
SET width = ?, height = ?, cells = ?
WHERE id = ?;

deleteById:
DELETE FROM World
WHERE id = ?;

findAll:
SELECT *
FROM World;

maxId:
SELECT MAX(id)
FROM World;